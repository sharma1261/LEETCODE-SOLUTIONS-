class Solution(object):
    def removeDuplicates(self, nums):
       
        if len(nums)<=1:                      #check the length of the nums if they are greater than 1 or not 
            return len(nums)      

        k=1                                   # first nums of the nums is always unique 

        for i in range(1,len(nums)):

            if nums[i]!=nums[i-1]:            #check if the previos number of the nums is equal to the current number 
                nums[k]=nums[i]
                k+=1
        return k
        
