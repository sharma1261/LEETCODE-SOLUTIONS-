class Solution(object):
    def twoSum(self, nums, target):
        
        seen={}
        for i,num in enumerate(nums):
            needed=target-num

            if needed in seen:
                return[seen[needed],i]

            seen[num]=i 
        return[]

s = Solution()

print(s.twoSum([2, 7, 11, 15], 9))  # [0, 1]
print(s.twoSum([3, 2, 4], 6))       # [1, 2]
print(s.twoSum([3, 3], 6))  #[0,1]
