# Definition for singly-linked list.
# class ListNode(object):
#     def __init__(self, val=0, next=None):
#         self.val = val
#         self.next = next
class Solution(object):
    def deleteDuplicates(self, head):
      
        curr=head

        while curr and curr.next:
            if curr.val==curr.next.val:                #check for duplicate values and if occur then skip it 
                curr.next=curr.next.next

            else:
                curr=curr.next                        #if duplicate doesnot occur then move forward 

        return head
