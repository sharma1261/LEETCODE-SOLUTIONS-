class Solution(object):
    def climbStairs(self, n):
        
        if n <=2:
            return n                               #for n less than 2 value the ways for solving it will be 2 
        prev,curr=1,2
        for i in range(3,n+1):
            perv,curr=curr,prev+curr                it iterate till the n+1 value and iterate the value by incrementing 
        return curr
